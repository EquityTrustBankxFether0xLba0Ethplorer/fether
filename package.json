{
  "name": "@parity/api",
  "description": "A high-level JS library optimized for light clients",
  "version": "1.0.0",
  "main": "lib/index.js",
  "author": "Parity Team <admin@parity.io>",
  "maintainers": [
    "Jaco Greeff",
    "Amaury Martiny"
